    "dev": "nodemon",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "author": "Sebastian Matias Segura (@seb7887)",
  "license": "ISC",
  "devDependencies": {
    "@nestjs/testing": "^6.11.6",
