    "@types/node": "^13.7.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@nestjs/core": "^6.11.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
